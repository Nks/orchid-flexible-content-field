{
  "name": "nakukryskin/orchid-flexible-content-field",
  "description": "Flexible Content field support for Orchid RAD platform",
  "license": "wtfpl",
  "authors": [
    {
      "name": "Nks",
      "email": "adm@progger.ru",
      "homepage": "https://progger.ru/",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/nakukryskin/orchid-flexible-content-field",
  "keywords": [
    "Laravel",
    "Orchid Page Builder",
    "Flexible content"
  ],
  "require": {
    "php": "^7.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "~3.0",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Nakukryskin\\OrchidFlexibleContentField\\": "src/"
