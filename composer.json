  },
  "autoload-dev": {
    "psr-4": {
      "Nakukryskin\\OrchidFlexibleContentField\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nakukryskin\\OrchidFlexibleContentField\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "RepeaterField": "Nakukryskin\\OrchidFlexibleContentField\\Facades\\OrchidFlexibleContentField"
      }
    }
  }
}
